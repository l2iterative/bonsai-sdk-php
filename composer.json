{
  "name": "l2iterative/bonsai-sdk-php",
  "description": "Bonsai SDK in PHP",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "require": {
    "guzzlehttp/guzzle": "^7.8",
    "php": ">=8.3"
  },
  "authors": [
    {
      "name": "Weikeng Chen",
      "email": "weikeng.chen@l2iterative.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "L2Iterative\\BonsaiSDK\\": "src/bonsai-sdk"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "L2Iterative\\BonsaiSDK\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "donatj/mock-webserver": "^2.7",
    "squizlabs/php_codesniffer": "*",
    "ramsey/uuid": "^4.7"
  },
  "scripts": {
    "phpcs": "vendor/bin/phpcs",
    "phpcbf": "vendor/bin/phpcbf",
    "phpdoc": "php -d error_reporting=0 phpDocumentor.phar"
  }
}
